{
  "name": "elv-live",
  "version": "3.0.0",
  "description": "React App for Eluvio Live",
  "main": "App.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-live",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8086 --host=0.0.0.0",
    "serve-https": "webpack-dev-server --https --hot --port 8086 --host=0.0.0.0",
    "build": "node ./node_modules/webpack-cli/bin/cli.js --mode=production --devtool false",
    "build-analyze": "ANALYZE_BUNDLE=true npm run build",
    "add-build": "git add dist",
    "lint": "npm-run-all -s -c lint-js-fix lint-scss-fix",
    "lint-js": "eslint ./src --ext js,jsx --report-unused-disable-directives",
    "lint-js-fix": "eslint ./src --fix --ext js,jsx --report-unused-disable-directives",
    "lint-scss": "npx stylelint \"src/**/*.{css,scss}\"",
    "lint-scss-fix": "npx stylelint \"src/**/*.{css,scss}\" --fix",
    "lint-main": "npm-run-all -s -c lint-js-fix-main lint-scss-fix-main",
    "lint-js-main": "eslint ./src/main --ext js,jsx --report-unused-disable-directives",
    "lint-js-fix-main": "eslint ./src/main --fix --ext js,jsx --report-unused-disable-directives",
    "lint-scss-main": "npx stylelint \"src/main/**/*.{css,scss}\"",
    "lint-scss-fix-main": "npx stylelint \"src/main/**/*.{css,scss}\" --fix"
  },
  "browser": {
    "crypto": false,
    "stream": false
  },
  "dependencies": {
    "@eluvio/elv-client-js": "^4.0.94",
    "@eluvio/elv-player-js": "^2.0.15",
    "@eluvio/elv-wallet-frame-client": "^2.1.4",
    "@paypal/react-paypal-js": "^4.0.2",
    "@react-hook/resize-observer": "^1.2.2",
    "@splidejs/react-splide": "^0.7.12",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@stripe/stripe-js": "^1.19.0",
    "country-codes-list": "^1.6.8",
    "dateformat": "^3.0.3",
    "dompurify": "^2.4.3",
    "ethers": "^5.3.0",
    "file-loader": "^6.2.0",
    "focus-trap-react": "^10.0.2",
    "isarray": "^2.0.5",
    "lodash": "^4.17.21",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-collapsible": "^2.8.1",
    "react-dom": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-inlinesvg": "^3.0.1",
    "react-markdown": "^8.0.5",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1",
    "rehype-raw": "^6.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-behavior-polyfill": "^2.0.13",
    "swiper": "^8.4.7",
    "url-join": "^4.0.1",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.22.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^4.6.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fast-sass-loader": "^2.0.1",
    "html-webpack-plugin": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^7.3.3",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "react-refresh": "^0.14.0",
    "regenerator-runtime": "^0.12.1",
    "sass": "^1.51.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.2.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "stylelint": "^16.8.2",
    "stylelint-config-sass-guidelines": "^12.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "yaml-loader": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eluv-io/elv-live-web.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/eluv-io/elv-live-web/issues"
  }
}
