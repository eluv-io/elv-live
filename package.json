{
  "name": "elv-live",
  "version": "2.0.12",
  "description": "React App for Eluvio Live",
  "main": "App.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-live-web",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8086 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "serve-https": "webpack-dev-server --https --cert https/private.pem --key https/private.key --hot --port 8086 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "build": "rm -rf dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js -p --colors --devtool none",
    "build-analyze": "rm -rf dist/* && NODE_ENV=production ANALYZE_BUNDLE=true node ./node_modules/webpack-cli/bin/cli.js -p --progress --colors --devtool none",
    "add-build": "git add dist",
    "lint-js": "eslint \"src/**/*.js\"",
    "fix-js": "eslint \"src/**/*.js\" --fix",
    "lint-scss": "stylelint '**/*.scss' --config stylelint.config.js",
    "fix-scss": "stylelint '**/*.scss' --config stylelint.config.js --fix",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint": "npm run fix-scss ; npm run fix-js",
    "test": "jest"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@eluvio/elv-client-js": "^3.1.71",
    "@eluvio/elv-player-js": "^1.0.63",
    "@eluvio/elv-wallet-client": "^0.1.16",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@paypal/react-paypal-js": "^4.0.2",
    "@react-hook/resize-observer": "^1.2.2",
    "@stripe/stripe-js": "^1.19.0",
    "bitmovin-player": "^8.50.0",
    "bitmovin-player-ui": "^3.22.0",
    "country-codes-list": "^1.6.8",
    "dateformat": "^3.0.3",
    "dayjs": "^1.10.4",
    "ethers": "^5.3.0",
    "getstream": "^6.2.2",
    "keccak": "^3.0.1",
    "lodash": "^4.17.21",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.7",
    "react": "^16.8.6",
    "react-add-to-calendar": "^0.1.5",
    "react-collapsible": "^2.8.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-inlinesvg": "^2.2.2",
    "react-markdown": "^5.0.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.4",
    "react-select": "^3.1.0",
    "react-share": "^4.4.0",
    "react-spinners": "^0.10.4",
    "react-twitter-embed": "^3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sanitize-html": "^2.3.3",
    "scroll-behavior-polyfill": "^2.0.13",
    "stream-chat": "^3.7.0",
    "stream-chat-react": "^5.1.7",
    "urijs": "^1.19.7",
    "url-join": "^4.0.1",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-regenerator": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "autoprefixer": "^9.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^26.6.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "rxjs": "^6.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.0",
    "yaml-loader": "^0.6.0"
  },
  "babel": {
    "comments": true,
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "mobx"
    ],
    "plugins": [
      "syntax-async-functions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-jsx",
      "@babel/plugin-syntax-object-rest-spread",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eluv-io/elv-live-web.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
