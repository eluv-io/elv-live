{
  "name": "elv-live-web",
  "version": "2.0.12",
  "description": "![Eluvio Logo](src/assets/images/logo/darkLogo.png \"Eluvio Logo\")",
  "main": "App.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-live-web",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8086",
    "serve-new": "webpack-dev-server --hot --port 8086 --entry ./src/App.js",
    "serve-phone": "webpack-dev-server --hot --port 8086 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "build": "rm -rf dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js -p --progress --colors --devtool none",
    "add-build": "git add dist",
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": ".bin/eslint \"src/**/*.js\"",
    "lint-scss": "sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "sass-lint-auto-fix/dist/index.js"
  },
  "pre-commit": {
    "run": [
      "lint",
      "build",
      "add-build"
    ]
  },
  "dependencies": {
    "@eluvio/elv-client-js": "^3.1.23",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@stripe/stripe-js": "^1.3.2",
    "@svgr/webpack": "^5.5.0",
    "antd": "^4.9.4",
    "axios": "^0.21.0",
    "bitmovin-player": "^8.50.0",
    "bitmovin-player-ui": "^3.22.0",
    "dashjs": "^3.0.3",
    "dateformat": "^3.0.3",
    "elv-components-js": "git+https://github.com/eluv-io/elv-components-js.git#645165da87ec42845a908a9ce0817db202523866",
    "getstream": "^6.2.2",
    "hls.js": "^0.12.4",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.7",
    "react": "^16.8.6",
    "react-add-to-calendar": "^0.1.5",
    "react-collapsible": "^2.8.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.4",
    "react-select": "^3.1.0",
    "react-twitter-embed": "^3.0.3",
    "styled-components": "^5.1.1",
    "urijs": "^1.19.1",
    "url-join": "^4.0.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-regenerator": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "autoprefixer": "^9.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "stream-chat": "^2.8.4",
    "stream-chat-react": "^3.2.4",
    "style-loader": "^0.22.1",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.0"
  },
  "babel": {
    "comments": true,
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "mobx"
    ],
    "plugins": [
      "syntax-async-functions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-jsx",
      "@babel/plugin-syntax-object-rest-spread",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-regenerator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eluv-io/elv-live-web.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/eluv-io/elv-live-web/issues"
  }
}
