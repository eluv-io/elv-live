    "@react-hook/resize-observer": "^1.2.2",
    "@stripe/stripe-js": "^1.19.0",
    "country-codes-list": "^1.6.8",
    "dateformat": "^3.0.3",
    "dayjs": "^1.10.4",
    "ethers": "^5.3.0",
    "isarray": "^2.0.5",
    "lodash": "^4.17.21",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-collapsible": "^2.8.1",
    "react-dom": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-inlinesvg": "^3.0.1",
    "react-markdown": "^5.0.3",
    "react-router": "^6.6.2",
    "react-router-dom": "^6.6.2",
