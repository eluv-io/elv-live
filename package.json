{
  "name": "elv-live",
  "version": "3.0.0",
  "description": "React App for Eluvio Live",
  "main": "App.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-live-web",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8086 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "serve-https": "webpack-dev-server --https --hot --port 8086 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "build": "rm -rf dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js -p --colors --devtool none",
    "build-analyze": "rm -rf dist/* && NODE_ENV=production ANALYZE_BUNDLE=true node ./node_modules/webpack-cli/bin/cli.js -p --progress --colors --devtool none",
    "add-build": "git add dist",
    "lint-js": "eslint \"src/**/*.js\"",
    "fix-js": "eslint \"src/**/*.js\" --fix",
    "lint-scss": "stylelint '**/*.scss' --config stylelint.config.js",
    "fix-scss": "stylelint '**/*.scss' --config stylelint.config.js --fix",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint": "npm run fix-scss ; npm run fix-js",
    "test": "jest"
  },
  "dependencies": {
    "@eluvio/elv-client-js": "^4.0.2",
    "@eluvio/elv-player-js": "^1.0.75",
    "@eluvio/elv-wallet-frame-client": "^2.0.2",
